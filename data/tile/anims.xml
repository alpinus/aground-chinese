		<frame x="3" offsetX="-5" count="2" />
		<frame x="5" count="5" />
	</animation>
	<animation id="equipment.stone_pickaxe" shift="2" >
		<frame x="0" offsetX="-4" />
		<frame x="1" offsetX="-7" />
		<frame x="2" offsetX="-8" />
		<frame x="3" offsetX="-4" count="2" />
		<frame x="5" count="5" />
	</animation>
	<animation id="equipment.iron_pickaxe" shift="2" >
		<frame x="0" offsetX="-4" />
		<frame x="1" offsetX="-7" />
		<frame x="2" offsetX="-3" count="3" />
		<frame x="5" count="5" />
	</animation>
	<animation id="equipment.steel_pickaxe" equals="equipment.iron_pickaxe" />
