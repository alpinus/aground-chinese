	</animation>
	<animation id="rat.walk" count="8" x="13" />
	
	<animation id="boar.idle" count="9">
		<frame count="2" />
		<frame x="1" y="2" />
		<frame x="3" y="0" count="6" />
		<frame count="9" />
		<frame count="9" />
	</animation>
