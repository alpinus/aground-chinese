		<frame x="5" count="5" />
	</animation>
	<animation id="equipment.bow">
		<frame x="4" count="2" />
		<frame x="0" count="2" length="3" />
		<frame x="2" count="2" />
	</animation>
	<animation id="equipment.walk">
		<frame x="0" />
		<frame x="0" offsetX="-1" length="3" />
		<frame x="0" length="2" />
		<frame x="0" offsetX="-1" length="3" />
		<frame x="0" />
	</animation>
	
	<animation id="rat.idle" count="8">
		<frame x="8" />
